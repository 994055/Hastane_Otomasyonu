@using Eczane.Models.Entity
@model Eczane.Models.personelList
@{
    ViewBag.Title = "personelDetay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container container-int">

    <ol class="breadcrumb">
        <li>@Html.ActionLink("Anasayfa/", "Anasayfa", "Eczaci")</li>
        <li>@Html.ActionLink("Personeller", "PersonelListele", "Yonetici")</li>
        <li class="active">/Personel Detay</li>
    </ol>

    <table class="table table-hover">
        <tbody>
            <tr class="table-primary">
                <th scope="row"> Adı Soyadı </th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.ad) @Html.DisplayFor(modelItem => personel.soyad)</a>
                    }
                </td>
            </tr>
        </tbody>
        <tbody>
            <tr class="table-primary">
                <th scope="row">TC</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.tc)</a>
                    }
                </td>

            </tr>
            <tr class="table-primary">
                <th scope="row">Adres</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.adres)</a>
                    }
                </td>

            </tr>
            <tr class="table-primary">
                <th scope="row">Cep Telefonu</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.ceptel)</a>
                    }
                </td>

            </tr>
            <tr class="table-primary">
                <th scope="row">Diploma No</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.diplomano)</a>
                    }
                </td>
            </tr>
            <tr class="table-primary">
                <th scope="row">Mezuniyet</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.mezuniyet)</a>
                    }
                </td>

            </tr>
            <tr class="table-primary">
                <th scope="row">Doğum Tarih</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        string tarih = personel.dtarih.ToString();
                        var dizi = tarih.Split(' ');
                        <a href="#" class="list-group-item list-group-item-action">@dizi[0]</a>
                    }
                </td>

            </tr>

            <tr class="table-primary">
                <th scope="row">Cinsiyet</th>
                <td>
                    @foreach (var personel in Model.personelListesi)
                    {
                        <a href="#" class="list-group-item list-group-item-action">@Html.DisplayFor(modelItem => personel.cinsiyet)</a>
                    }
                </td>

            </tr>

        </tbody>
    </table>

</div>


